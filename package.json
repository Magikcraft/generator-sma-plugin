{
	"name": "generator-sma-plugin",
	"version": "0.0.17",
	"description": "Scaffold a package for the Scriptcraft Modular Architecture - Minecraft Plugins in Javascript!",
	"homepage": "https://www.magikcraft.io",
	"author": {
		"name": "Josh Wulf",
		"email": "josh@magikcraft.io",
		"url": "https://www.github.com/jwulf"
	},
	"files": [
		"generators"
	],
	"main": "generators/index.js",
	"keywords": [
		"minecraft",
		"scriptcraft",
		"magikcraft",
		"yeoman-generator"
	],
	"devDependencies": {
		"yeoman-test": "^1.7.0",
		"yeoman-assert": "^3.1.0",
		"eslint": "^5.9.0",
		"prettier": "^1.15.2",
		"husky": "^1.2.0",
		"lint-staged": "^13.1.0",
		"eslint-config-prettier": "^3.3.0",
		"eslint-plugin-prettier": "^3.0.0",
		"eslint-config-xo": "^0.25.0",
		"jest": "^23.5.0"
	},
	"engines": {
		"npm": ">= 4.0.0"
	},
	"dependencies": {
		"@types/inquirer-npm-name": "^3.0.0",
		"@types/lodash": "^4.14.123",
		"@types/mkdirp": "^0.5.2",
		"chalk": "^2.1.0",
		"generator-git-init": "^1.1.3",
		"generator-node": "^2.6.0",
		"inquirer-npm-name": "^3.0.0",
		"lodash": "^4.17.11",
		"mkdirp": "^0.5.1",
		"yeoman-generator": "^2.0.1",
		"yosay": "^2.0.1"
	},
	"jest": {
		"testEnvironment": "node"
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"git add"
		],
		"*.json": [
			"prettier --write",
			"git add"
		]
	},
	"eslintConfig": {
		"extends": [
			"xo",
			"prettier"
		],
		"env": {
			"jest": true,
			"node": true
		},
		"rules": {
			"prettier/prettier": "error"
		},
		"plugins": [
			"prettier"
		]
	},
	"scripts": {
		"pretest": "eslint .",
		"test": "jest"
	},
	"repository": "Magikcraft/generator-sma-plugin",
	"license": "Apache-2.0"
}
